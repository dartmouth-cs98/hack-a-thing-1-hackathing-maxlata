{
    "name": "Sneaky YouTube List",
    "version": "1.0",
    "description": "Add YouTube videos with descriptions to a list we can cast",
    "manifest_version": 2,
    "background": {
      "page": "background.html",
      "matches": ["https://youtube.com/watch*", "http://youtube.com/watch*"],
      "persistent": true
    },
    "permissions": [
      "tabs",
      "identity",
      "https://www.gstatic.com/firebasejs/6.3.5/firebase.js"
    ],
    "browser_action": {
      "default_icon": {
        "16": "images/icon16.png"
      },
      "default_popup": "popup.html"
    },
    "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://www.gstatic.com/firebasejs/6.6.2/firebase-app.js 'sha256-ZsuMvM8P5M/UdmXHmlKXbq6LZm47FIp7unfagjyrWFg=' 'sha256-C2GEvBKHUZfh5fkd+2eUrZBO1mRpDQYKDxxETqtHO20='; object-src 'self'",
    "oauth2": {
      "client_id": "126497100275-l4h012h5ob47u84q0h4i7umaf4d9kbrg.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    },
    "key": ""
}
